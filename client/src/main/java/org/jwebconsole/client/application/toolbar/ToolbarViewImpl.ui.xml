<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:toolbar="urn:import:com.sencha.gxt.widget.core.client.toolbar"
             xmlns:button="urn:import:com.sencha.gxt.widget.core.client.button">

    <ui:with field="messages" type="org.jwebconsole.client.bundle.messages.Messages"/>
    <ui:with field="icons" type="org.jwebconsole.client.bundle.AppIcons"/>

    <toolbar:ToolBar ui:field="toolbar">
        <button:TextButton ui:field="createConnectionButton" icon="{icons.iconAdd}"/>
        <button:TextButton ui:field="editConnectionButton" icon="{icons.iconEdit}"/>
        <button:TextButton ui:field="deleteConnectionButton" icon="{icons.iconDelete}"/>
    </toolbar:ToolBar>

</ui:UiBinder>