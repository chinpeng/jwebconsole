<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:toolbar="urn:import:com.sencha.gxt.widget.core.client.toolbar"
             xmlns:button="urn:import:com.sencha.gxt.widget.core.client.button">

    <ui:with field="res" type="org.jwebconsole.client.bundle.AppResources"/>
    <ui:with field="messages" type="org.jwebconsole.client.bundle.messages.Messages"/>

    <toolbar:ToolBar ui:field="toolbar">
        <button:TextButton text="{messages.createConnectionToolbarButtonText}" ui:field="createConnectionButton"/>
    </toolbar:ToolBar>

</ui:UiBinder>