<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container"
        xmlns:client="urn:import:com.sencha.gxt.widget.core.client"
        xmlns:button="urn:import:com.sencha.gxt.widget.core.client.button"
        xmlns:grid="urn:import:com.sencha.gxt.widget.core.client.grid">

    <ui:with field="messages" type="org.jwebconsole.client.bundle.messages.Messages"/>

    <container:PortalLayoutContainer numColumns="2" ui:field="portal">

        <container:portlet column="0">
            <client:Portlet headingText="{messages.tabMemoryHeaderText}" height="320" collapsible="true" animCollapse="false" ui:field="threadChartPortlet">

            </client:Portlet>
        </container:portlet>

        <container:portlet column="1">
            <client:Portlet headingText="{messages.tabThreadsHeaderText}" height="320" collapsible="true" animCollapse="false" ui:field="threadChartPortlet">

            </client:Portlet>
        </container:portlet>

        <container:portlet column="0">
            <client:Portlet headingText="{messages.tabThreadsHeaderText}" height="320" collapsible="true" animCollapse="false">

            </client:Portlet>
        </container:portlet>

        <container:portlet column="1">
            <client:Portlet headingText="{messages.tabThreadsHeaderText}" height="320" collapsible="true" animCollapse="false">

            </client:Portlet>
        </container:portlet>

    </container:PortalLayoutContainer>
</ui:UiBinder>
